{
    "manifest_version": 3,
    "name": "Tab Group Organizer",
    "version": "1.0",
    "permissions": [
        "tabs",
        "tabGroups",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
